.loading
  position fixed
  top 40%
  right 0
  left 0
  transform translateY(-40%)

  .typed-element
    width 100%
    display flex
    justify-content center
    user-select none

  .enter-directly
    text-align center
    span
      cursor pointer
      user-select none
      font-size 0.8rem
      font-weight bold
      transition opacity 1s

